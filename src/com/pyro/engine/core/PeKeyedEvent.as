package com.pyro.engine.core 
{
	/**
	 * ...
	 * @author Leland Ede
	 */
	public class PeKeyedEvent 
	{
		private var _keyCode:int;
		private var _functionArray:Array;
		private var _process:Boolean;
		
		public function PeKeyedEvent(code:int) 
		{
			_keyCode = code;
			_functionArray = [];
			_process = false;
		}
		
		public function get keyCode():int 
		{
			return _keyCode;
		}
		
		public function set keyCode(value:int):void 
		{
			_keyCode = value;
		}
		
		public function get functionArray():Array 
		{
			return _functionArray;
		}
		
		public function get process():Boolean 
		{
			return _process;
		}
		
		public function set process(value:Boolean):void 
		{
			_process = value;
		}
	}

}