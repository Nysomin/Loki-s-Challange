	public class InKeyArray 
	{
		private var _funcArray:Array;
		private var _code:int;
		private var _process:Boolean;
		
		public function InKeyArray(code:int) 
		{
			_funcArray = [];
			_code = code;
			_process = false;
		}
		
		public function get funcArray():Array 
		{
			return _funcArray;
		}
		
		public function set funcArray(value:Array):void 
		{
			_funcArray = value;
		}
		
		public function get code():int 
		{
			return _code;
		}
		
		public function set code(value:int):void 
		{
			_code = value;
		}
		
		public function get process():Boolean 
		{
			return _process;
		}
		
		public function set process(value:Boolean):void 
		{
			_process = value;
		}
		
	}

}