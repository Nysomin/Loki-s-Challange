package com.pyro.engine.gfx2d.objects 
{
	import com.pyro.engine.objects.PePoint;
	/**
	 * ...
	 * @author Leland Ede
	 */
	public class PeSpawnObject 
	{
		private var _pos:PePoint;
		private var _type:int;
		
		public function PeSpawnObject(inPos:PePoint, inType:int) 
		{
			pos = inPos;
			type = inType;
		}
		
		public function get pos():PePoint 
		{
			return _pos;
		}
		
		public function set pos(value:PePoint):void 
		{
			_pos = value;
		}
		
		public function get type():int 
		{
			return _type;
		}
		
		public function set type(value:int):void 
		{
			_type = value;
		}
		
	}

}