	public class PeVector2D 
	{
		private var _x:Number;
		private var _y:Number;
		
		public function PeVector2D(inX:Number = 0.0, inY:Number = 0.0) 
		{
			_x = inX;
			_y = inY;
		}
		
		public function setXY(inX:Number, inY:Number):void
		{
			x = inX;
			y = inY;
		}
		
		public function get x():Number 
		{
			return _x;
		}
		
		public function set x(value:Number):void 
		{
			_x = value;
		}
		
		public function get y():Number 
		{
			return _y;
		}
		
		public function set y(value:Number):void 
		{
			_y = value;
		}
		public function toString():String
		{
			var out:String = "[" + _x + ", " + _y + "]";
			return out;
		}
		
	}

}